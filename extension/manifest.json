{
  "name": "Tabs Drawer Extension",
  "manifest_version": 2,
  "description": "An extension that helps you keep your tabs under control",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Tas drawer"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
  "incognito":"split",
  "content_security_policy": "script-src 'self' 'sha256-1DEQrcjWua4d1jiU5X4gB/h+PBwzdU/SCfVByEuTbTg=' 'sha256-YN6y+RK5oBVcLPEOqqVcYOvmOF1WnUC/K56wU0k8fwc='"
}
